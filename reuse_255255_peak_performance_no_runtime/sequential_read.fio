size=100%
filename=/dev/sdd
rw=read
iodepth=21

[disk3]
bs=1m
size=100%
filename=/dev/sde
rw=read
iodepth=21

[disk4]
bs=1m
size=100%
filename=/dev/sdf
rw=read
