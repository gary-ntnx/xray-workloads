[global]
ioengine=libaio
direct=1
time_based
norandommap
group_reporting
disk_util=0
continue_on_error=all
rate_process=poisson
iomem=shmhuge

[disk0]
bs=1m
size=100%
filename=/dev/sdb
rw=read
iodepth=21

